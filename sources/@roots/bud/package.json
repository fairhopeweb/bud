{
  "name": "@roots/bud",
  "version": "0.0.0",
  "description": "Frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "homepage": "https://roots.io/bud",
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "clayrisser",
      "url": "https://github.com/clayrisser"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "roots",
    "webpack"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "bin/",
    "lib/",
    "types/",
    "vendor/"
  ],
  "bin": "./bin/bud.mjs",
  "type": "module",
  "module": "./lib/esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./lib/esm/index.js"
      }
    },
    "./cli": {
      "import": {
        "types": "./types/cli/index.d.ts",
        "default": "./lib/esm/cli/index.js"
      }
    },
    "./cli/commands": {
      "import": {
        "types": "./types/cli/commands/index.d.ts",
        "default": "./lib/esm/cli/commands/index.js"
      }
    },
    "./cli/commands/*": {
      "import": {
        "types": "./types/cli/commands/*.d.ts",
        "default": "./lib/esm/cli/commands/*.js"
      }
    },
    "./context": {
      "import": {
        "types": "./types/context/index.d.ts",
        "default": "./lib/esm/context/index.js"
      }
    },
    "./context/*": {
      "import": {
        "types": "./types/context/*.d.ts",
        "default": "./lib/esm/context/*.js"
      }
    },
    "./extensions": {
      "import": {
        "types": "./types/extensions/index.d.ts",
        "default": "./lib/esm/extensions/index.js"
      }
    },
    "./extensions/*": {
      "import": {
        "types": "./types/extensions/*/index.d.ts",
        "default": "./lib/esm/extensions/*/index.js"
      }
    },
    "./factory": {
      "import": {
        "types": "./types/factory/index.d.ts",
        "default": "./lib/esm/factory/index.js"
      }
    },
    "./services": {
      "import": {
        "types": "./types/services/index.d.ts",
        "default": "./lib/esm/services/index.js"
      }
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/braces": "3.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "^4",
    "@types/mini-css-extract-plugin": "2.5.0",
    "@types/node": "16.11.36",
    "@types/node-notifier": "8.0.2",
    "@types/signale": "1.4.4"
  },
  "dependencies": {
    "@roots/bud-api": "workspace:sources/@roots/bud-api",
    "@roots/bud-build": "workspace:sources/@roots/bud-build",
    "@roots/bud-cache": "workspace:sources/@roots/bud-cache",
    "@roots/bud-compiler": "workspace:sources/@roots/bud-compiler",
    "@roots/bud-dashboard": "workspace:sources/@roots/bud-dashboard",
    "@roots/bud-extensions": "workspace:sources/@roots/bud-extensions",
    "@roots/bud-framework": "workspace:sources/@roots/bud-framework",
    "@roots/bud-hooks": "workspace:sources/@roots/bud-hooks",
    "@roots/bud-server": "workspace:sources/@roots/bud-server",
    "chalk": "5.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "clipanion": "3.2.0-rc.11",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "16.0.1",
    "dotenv-expand": "8.0.3",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "globby": "13.1.1",
    "helpful-decorators": "2.1.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash-es": "4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "node-notifier": "10.0.1",
    "open": "8.4.0",
    "open-editor": "4.0.0",
    "pkg-up": "4.0.0",
    "pretty-format": "28.1.0",
    "signale": "1.4.0",
    "table": "6.8.0",
    "tslib": "2.4.0",
    "typanion": "3.9.0",
    "webpack": "5.72.1",
    "webpack-cli": "^4.9.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-remove-empty-scripts": "0.8.0"
  }
}
