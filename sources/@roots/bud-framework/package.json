{
  "name": "@roots/bud-framework",
  "version": "0.0.0",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud-framework"
  },
  "contributors": [
    {
      "name": "kellymears",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js"
    },
    "./bud": {
      "types": "./types/bud.d.ts",
      "import": "./lib/bud.js"
    },
    "./extension": {
      "types": "./types/extension/index.d.ts",
      "import": "./lib/extension/index.js"
    },
    "./extension/decorators": {
      "types": "./types/extension/decorators/index.d.ts",
      "import": "./lib/extension/decorators/index.js"
    },
    "./extension/decorators/*": {
      "types": "./types/extension/decorators/*.d.ts",
      "import": "./lib/extension/decorators/*.js"
    },
    "./methods/*": {
      "types": "./types/methods/*.d.ts",
      "import": "./lib/methods/*.js"
    },
    "./module": {
      "types": "./types/module.d.ts",
      "import": "./lib/module.js"
    },
    "./service": {
      "types": "./types/service/index.d.ts",
      "import": "./lib/service/index.js"
    },
    "./services/*": {
      "types": "./types/services/*/index.d.ts",
      "import": "./lib/services/*/index.js"
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/fs-extra": "9.0.13",
    "@types/js-yaml": "^4",
    "@types/lodash": "4.14.182",
    "@types/lodash-es": "^4",
    "@types/node": "16.11.36",
    "@types/signale": "1.4.4",
    "chokidar": "3.5.3",
    "http-proxy-middleware": "2.0.6",
    "webpack": "5.72.1",
    "webpack-dev-middleware": "5.3.3",
    "webpack-hot-middleware": "2.25.1"
  },
  "dependencies": {
    "@roots/container": "workspace:sources/@roots/container",
    "boxen": "6.2.1",
    "cli-highlight": "2.1.11",
    "figures": "4.0.1",
    "fs-extra": "10.1.0",
    "globby": "13.1.1",
    "helpful-decorators": "2.1.0",
    "import-from": "4.0.0",
    "import-meta-resolve": "2.0.2",
    "js-yaml": "4.1.0",
    "json5": "2.2.1",
    "lodash-es": "4.17.21",
    "pkg-up": "4.0.0",
    "pretty-format": "28.1.0",
    "resolve-from": "5.0.0",
    "signale": "1.4.0",
    "tslib": "2.4.0"
  }
}
